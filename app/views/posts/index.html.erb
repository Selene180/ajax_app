<h1>AjaxApp</h1>
<%= form_with url:  "/posts", method: :post,id: "form" do |form| %>
  <%= form.text_field :content %>
  <%= form.submit '投稿する' , id: "submit" %>
<% end %>

<h1>Top page</h1>
<%= @post%>
<%= link_to "投稿ページ", '/posts/new'%>
<% @posts.each do |post| %>
  <div>
    <%= post.memo %>
    <%= post.created_at %>
  </div>
<% end %>